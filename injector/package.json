{
  "version": "1.0.2",
  "license": "MIT",
  "name": "@slimlib/injector",
  "author": "Konstantin Shutkin",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
