{
    "type": "module",
    "version": "1.6.2",
    "name": "@slimlib/store",
    "description": "Simple Proxy-based store for SPA",
    "license": "MIT",
    "author": "Konstantin Shutkin",
    "main": "./src/index.js",
    "module": "./src/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "default": "./src/index.js"
        },
        "./package.json": "./package.json"
    },
    "types": "./types/index.d.ts",
    "files": [
        "src",
        "types"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kshutkin/slimlib.git",
        "directory": "store"
    },
    "bugs": "https://github.com/kshutkin/slimlib/issues",
    "homepage": "https://github.com/kshutkin/slimlib/blob/main/store/README.md",
    "keywords": [
        "@slimlib",
        "store",
        "proxy"
    ],
    "scripts": {
        "build": "dts-buddy",
        "test": "vitest run --coverage --config ../vitest.config.mjs",
        "bench": "node tests/benchmark.mjs -n 50 -f ./results.csv",
        "build-compress": "pkgbld --formats=es --compress=es --no-update-package-json --include-externals",
        "prepack": "pkgprn"
    },
    "dependencies": {
        "@slimlib/list": "workspace:^",
        "esm-env": "^1.2.1"
    },
    "devDependencies": {
        "@angular/core": "^19.0.0",
        "@preact/signals-core": "^1.8.0",
        "@reactively/core": "^0.0.8",
        "@vue/reactivity": "^3.5.0",
        "alien-signals": "^1.0.13",
        "pkgbld": "1.36.0",
        "rxjs": "^7.8.0",
        "solid-js": "^1.9.5",
        "svelte": "^5.0.0"
    }
}
