{
  "version": "1.3.3",
  "license": "MIT",
  "name": "@slimlib/store",
  "author": "Konstantin Shutkin",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./core": {
      "require": "./dist/core.cjs",
      "default": "./dist/core.mjs"
    },
    "./react": {
      "require": "./dist/react.cjs",
      "default": "./dist/react.mjs"
    },
    "./preact": {
      "require": "./dist/preact.cjs",
      "default": "./dist/preact.mjs"
    },
    "./svelte": {
      "require": "./dist/svelte.cjs",
      "default": "./dist/svelte.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist",
    "react",
    "preact",
    "core",
    "svelte",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshutkin/slimlib.git"
  },
  "bugs": {
    "url": "https://github.com/kshutkin/slimlib/issues"
  },
  "homepage": "https://github.com/kshutkin/slimlib/blob/main/store/README.md",
  "scripts": {
    "build": "pkgbld-internal --umd=index,preact,react,svelte",
    "test": "jest --collectCoverage",
    "lint": "eslint ./src"
  },
  "peerDependencies": {
    "preact": ">=10.0.0",
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "preact": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "preact": ">=10.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react-dom": ">=17.0.0",
    "jest-environment-jsdom": "^29.4.3",
    "@testing-library/svelte": "^3.2.2",
    "svelte": "^3.55.1",
    "svelte-jester": "^2.3.2",
    "tslib": "^2.5.0"
  },
  "description": "Simple Proxy-based store for SPA",
  "keywords": [
    "@slimlib",
    "store",
    "proxy"
  ]
}
